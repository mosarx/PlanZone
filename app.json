{
  "expo": {
    "name": "PlanZone",
    "slug": "PlanZone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/otrac.png",
    "userInterfaceStyle": "light",
    "scheme": "planzone",
    "splash": {
      "image": "./assets/images/bosar.png",
      "resizeMode": "cover",
      "backgroundColor": "#FAB0B0"  // couleur de fond "safe area"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.southpark.planzone",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/otrac.png",
        "backgroundColor": "#FAB0B0"
      },
      "package": "com.southpark.planzone",
      "edgeToEdgeEnabled": true
    },
    "extra": {
      "eas": {
        "projectId": "695cc0e8-acd1-4fbf-b756-4c802a92ce08"
      }
    },
    "plugins": [
      "expo-build-properties",
      "expo-web-browser",
      "expo-font"
    ],
    "owner": "mosarx"
  }
}
