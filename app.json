{
  "expo": {
    "name": "PlanZone",
    "slug": "PlanZone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mura.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "planzone",           // ðŸ‘ˆ ajoutÃ© ici
    "splash": {
      "image": "./assets/images/urus.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.southpark.planzone"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mura.png"
      },
      "package": "com.southpark.planzone",
      "edgeToEdgeEnabled": true
    },
    "extra": {
      "eas": {
        "projectId": "695cc0e8-acd1-4fbf-b756-4c802a92ce08"
      }
    },
    "plugins": [
      "expo-build-properties",
      "expo-web-browser"
    ]
  }
}
